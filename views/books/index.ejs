<h3>Search options Books List</h3>
<form action="/books" method="GET">
    <div class="form-row">
        <div class="form-item">
            <label for="title">Title</label>
            <input type="text" name="title" id="title" value="<%= searchoptions.title %>">
        </div>
        <div class="form-item">
            <label for="publishbefore">Publish Before</label>
            <input type="date" name="publishbefore" id="publishbefore" value="<%= searchoptions.publishbefore %>">
        </div>
    </div>
    <div class="form-row">
        <div class="form-item">
            <label for="publishafter">Publish After</label>
            <input type="date" name="publishafter" id="publishafter" value="<%= searchoptions.publishafter %>">
        </div>
    </div>
    <div class="form-row btn-row">
        <button type="submit" class="btn btn-primary">Search</button>
        <a href="" class="btn btn-danger">Cancel</a>
    </div>

</form>
<%- include('./partials/gridview') %>