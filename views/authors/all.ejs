<h3>All Authors Name List</h3>
<ul>
    <% authors.forEach((auth)=>{ %>
        <div class="author-row">
    <li class="list-data"><%= auth.name %></li>
    <div class="btn-row">
        <a href="/authors/<%= auth.id %>" class="btn btn-primary">Edit</a>
        <%- include ('./partials/delete',{url:`/authors/${auth.id}`}) %>
    </div>
</div>
    <% }) %>
</ul>
